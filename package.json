{
  "name": "teknofest",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:routes-gen": "npm run routes-gen",
    "build:prisma": "prisma generate",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:routes-gen": "npm run routes-gen -- --watch",
    "dev:prisma": "prisma generate --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "db:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "db:reset": "prisma migrate reset --force --skip-generate",
    "db:studio": "prisma studio --browser=none",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:repl": "prisma-repl",
    "start": "remix-serve build",
    "cleanup": "run-p db:reset build",
    "preplaywright": "env-cmd -f .env.e2e npm run cleanup",
    "playwright": "env-cmd -f .env.e2e playwright test",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "cross-env CI=true run-p test lint typecheck playwright",
    "routes-gen": "routes-gen -d @routes-gen/remix"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report"
  ],
  "dependencies": {
    "@mantine/core": "^4.2.6",
    "@mantine/dates": "^4.2.6",
    "@mantine/dropzone": "^4.2.6",
    "@mantine/hooks": "^4.2.6",
    "@mantine/prism": "^4.2.6",
    "@mantine/ssr": "^4.2.6",
    "@prisma/client": "^3.11.1",
    "@remix-run/node": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "@remix-run/server-runtime": "^1.4.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "routes-gen": "^0.5.0",
    "tabler-icons-react": "^1.48.1",
    "ts-expect": "^1.3.0",
    "ts-node": "^10.7.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@remix-run/dev": "^1.4.1",
    "@remix-run/eslint-config": "^1.4.1",
    "@routes-gen/remix": "^0.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@vitejs/plugin-react": "^1.3.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prisma": "^3.11.1",
    "prisma-dbml-generator": "^0.9.1",
    "prisma-repl": "^1.3.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.9"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
