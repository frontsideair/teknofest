{
  "name": "teknofest",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:routes-gen": "npm run routes-gen",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:routes-gen": "npm run routes-gen -- --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "db:setup": "prisma migrate dev && prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio --browser=none",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "start": "remix-serve build",
    "test": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "routes-gen": "routes-gen -d @routes-gen/remix"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@mantine/core": "^4.2.1",
    "@mantine/dates": "^4.2.5",
    "@mantine/hooks": "^4.2.1",
    "@mantine/prism": "^4.2.5",
    "@mantine/ssr": "^4.2.1",
    "@prisma/client": "^3.11.1",
    "@remix-run/node": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "@remix-run/server-runtime": "^1.4.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "routes-gen": "^0.4.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@playwright/test": "^1.22.2",
    "@remix-run/dev": "^1.4.1",
    "@remix-run/eslint-config": "^1.4.1",
    "@routes-gen/remix": "^0.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^2.55.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "prisma": "^3.11.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.8.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
